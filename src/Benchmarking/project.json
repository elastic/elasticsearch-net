{
  "version": "5.0.0-alpha2",
  "buildOptions": {
    "warningsAsErrors": false,
    "define": [ "LINQ", "NDESK_OPTIONS" ],
    "emitEntryPoint": true
  },
  "dependencies": {
    "Nest": { "target": "project" },
    "Elasticsearch.Net": { "target": "project" },
    "Tests": { "target": "project" },
    "Humanizer": "2.0.0-beta0001"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "commands": {
    "Benchmarking": "Benchmarking"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Diagnostics.Tools": "",
        "System.IO.Compression.FileSystem": "",
        "System.Reflection": "",
        "System.Runtime": "",
        "System.Threading.Tasks": ""
      },
      "dependencies": {
      }
    },
    "netcoreapp1.0": {
      "buildOptions": { "define": [ "DOTNETCORE" ] },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-rc2-3002702"
        }
      },
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}