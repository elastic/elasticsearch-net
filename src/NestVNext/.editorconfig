[*.cs]
resharper_check_namespace_highlighting=do_not_show
resharper_inheritdoc_invalid_usage_highlighting=do_not_show
# nest uses a lot of generated partial code, for clearity we want to duplicate extends definitions
resharper_redundant_extends_list_entry_highlighting=do_not_show

# CS3003: Type is not CLS-compliant
dotnet_diagnostic.CS3003.severity = suggestion
